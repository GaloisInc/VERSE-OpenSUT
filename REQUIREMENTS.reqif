            <ATTRIBUTE-VALUE-STRING THE-VALUE="Nested types">
              <DEFINITION>
                <ATTRIBUTE-DEFINITION-STRING-REF>ReqIF.Name</ATTRIBUTE-DEFINITION-STRING-REF>
              </DEFINITION>
            </ATTRIBUTE-VALUE-STRING>
            <ATTRIBUTE-VALUE-STRING THE-VALUE="VERSE Toolchain shall support reasoning about structs composed of other structs.">
